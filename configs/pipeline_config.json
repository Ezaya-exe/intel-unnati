{
  "pipeline": {
    "ocr": {
      "use_gpu": false,
      "supported_languages": [
        "english",
        "hindi",
        "sanskrit",
        "marathi"
      ]
    },
    "chunking": {
      "chunk_size": 512,
      "chunk_overlap": 128
    },
    "embedding": {
      "model": "paraphrase-multilingual-mpnet-base-v2",
      "batch_size": 32
    },
    "llm": {
      "provider": "huggingface",
      "model": "google/flan-t5-base",
      "temperature": 0.7,
      "max_tokens": 500
    }
  },
  "subjects": [
    "Mathematics",
    "Science",
    "Social Science",
    "Hindi",
    "Sanskrit",
    "English"
  ],
  "grades": [
    5,
    6,
    7,
    8,
    9,
    10
  ]
}